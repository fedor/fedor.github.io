{
  "name": "@fedor/fedor.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "scripts": {
    "build": "node build.js && node ./node_modules/tailwindcss/lib/cli.js -i ./src/input.css -o ./build/style.css && node ./node_modules/tailwindcss/lib/cli.js -c ./tailwind.config.noscript.js -i ./src/input.css -o ./build/style.noscript.css",
    "live-build": "./node_modules/nodemon/bin/nodemon.js -w ./src -w ./build.js -e html,json,js,css -x \"npm run build\"",
    "live-server": "node server.js",
    "start": "parallel --ungroup npm run ::: live-build live-server"
  },
  "keywords": [],
