  "dependencies": {
    "html-minifier": "^3.4.2",
    "live-server": "^1.2.2",
    "marked": "^0.3.6",
    "mustache": "^2.3.0",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.5"
  }
}
